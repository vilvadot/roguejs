{
    "compilerOptions": {
      "target": "ES2017" ,
      "esModuleInterop": true ,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "noImplicitAny": false,
      "outDir": "src/dist",
      "allowJs": true,
      "types": ["vitest"]
    },
    "exclude": [
      "node_modules",
      "src/dist",
      "**/*/*.test.ts",
      "src/lib/**/*/*.ts"
    ]
  }
  