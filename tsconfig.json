{
  "compilerOptions": {
    "target": "ES2015" ,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "outDir": "src/dist",
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "src/dist",
    "**/*/*.test.ts",
    "src/lib/**/*/*.ts"
  ]
}
