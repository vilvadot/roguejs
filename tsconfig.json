{
    "compilerOptions": {
      "target": "ES2017" ,
      "esModuleInterop": true ,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "noImplicitAny": false,
      "outDir": "dist",
      "allowJs": true,
      "types": ["vitest"],
      "moduleResolution": "node"
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules",
      "dist",
    ]
  }
  